{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "my": true,
    "App": true,
    "Component": true,
    "Page": true
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false,
    "ecmaVersion": "latest",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "warn",
    "no-undef": "off"
  }
}